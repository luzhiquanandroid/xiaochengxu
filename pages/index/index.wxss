/* 小转盘 */


.canvas-container ul,
.canvas-container li{
  margin: 0 ;
  padding: 0;
  list-style: none;
}

.canvas-container{
  margin: 0 auto;
  position: relative;
  width: 600rpx;
  height: 600rpx;  
  border-radius: 50%;
  top:26rpx;

}
.canvas-content{
  position: absolute;
  left: 0;
  top: 0;
  z-index: 1;
  display: block;
  width: 600rpx;
  height: 600rpx;
  border-radius: inherit;
  background-clip: padding-box;
  background-color: #fff;
}
.canvas-element{
  position: relative;
  z-index: 1;
  width: inherit;
  height: inherit;
  border-radius: 50%;
}
.canvas-list{
  position: absolute;
  left: 0;
  top: 0;
  width: inherit;
  height: inherit;
  z-index: 9999;
}
.canvas-item{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  color: #A97237;
  font-weight: bold;
  font-size: 30rpx;
}
.canvas-item-text{
  position: relative;
  display: block;
  padding-top: 50rpx;
  margin: 0 auto;
  text-align: center; 
  -webkit-transform-origin: 50% 300rpx; 
  transform-origin: 50% 300rpx;
}
.turntableBt text{
    color:#fff;
    position: absolute;
    font-size:36rpx;
    top:50%;
    left:50%;
    margin-left:-38rpx;
    margin-top:5rpx;


}
.getInfoBtn{
    width:161rpx;
    left:50%;
    margin-left: -80rpx;
    z-index:999999
}
/* 分隔线 */
.canvas-line{
  position: absolute;
  left: 0;
  top: 0;
  width: inherit;
  height: inherit;
  z-index: 99;
}
.canvas-litem{
  position: absolute;
   left: 300rpx;
   top: 0;
   width: 1rpx;
   height: 300rpx;
   background-color:#f7ae4f;
   overflow: hidden; 
   -webkit-transform-origin: 50% 300rpx; 
   transform-origin: 50% 300rpx;
}

.gb-run{ 
    -webkit-transition: all 6s ease;
    transition: all 6s ease;
}


