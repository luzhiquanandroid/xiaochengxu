    float: left;
    width:320rpx;
}
.headImageBox{
    width: 109rpx;
    height: 109rpx;
    margin:0 auto;
    margin-top:40rpx;
}

.userinfo-headImage{
    /* position: absolute; */
   
    width: 106rpx;
    height: 106rpx;
    border-radius: 50%;
    position: relative;
    z-index:99
}
.headImage{
    position: absolute;
    width: 70rpx;
    height: 70rpx;
