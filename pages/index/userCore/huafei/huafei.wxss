/*话费总额与钻石总额+提取话费样式*/
.zuanMoney{
    float: right;
    position: relative;
    z-index: 99;
    margin-right:52rpx;
    text-align:right;
}
.phoneCharges{
    font-size:32rpx;
    color:#F6DD45;
    font-weight: bold;
    margin-top:56rpx;
    margin-bottom:15rpx;
}
.phoneCharges text{
    font-size:60rpx;
    margin-right:10rpx;
}
.zuanQuota{
    font-size:20rpx;
    color:#F6DD45;
    font-weight: bold;
}
.zuanQuota text{
    font-size:36rpx;
    margin-right:10rpx;
}
.extractBtn{
    width:200rpx;
    height:55rpx;
    background:#F6DD45;
    border-radius: 30rpx;
    color:#C11024;
    font-size:26rpx;
    line-height: 55rpx;
    margin-top:39rpx;
    margin-right:0;
    font-weight: bold;
}
/*个人中心列表*/
.listCont{
    position: absolute;
    top:320rpx;
    width:100%;
}
.list{
    width:100%;
    height:150rpx;
    line-height: 100rpx;
    background:#fff;
    margin-bottom:10rpx;
    overflow: hidden;
    margin-top:10rpx;
    position:relative;
}

.listTitle{
    font-size:24rpx;
   margin-left:51rpx;
    color:#333333;
    position: absolute;
    top:-17rpx;
    left:0rpx;
}
.listNum{
   margin-left:51rpx; 
   overflow: hidden;
   margin-top:40rpx;
}
.listNum view{
  display: inline-block;
  width:103rpx;
  height:46rpx;
  margin-right:45rpx;
  border:1px solid #d5d5d5;
  font-size:24rpx;
  color:#d5d5d5;
  line-height: 46rpx;
  text-align:center;
  border-radius:10rpx;
}
.listNum view.active{
   border:1px solid #FF5151;
  color:#FF5151;
}
.listNum input{
  border-bottom:1px solid #D8D8D8;
  margin-right: 51rpx;
  font-size:20rpx;
  height:54rpx;
  line-height: 54rpx;
  margin-top:35rpx;
}
.listNum input.bigFont{
    font-size: 28rpx;
}
.tiBtn{
  width:674rpx;
  height:80rpx;
  line-height:80rpx;
  text-align:center;
  color:#fff;
  margin:0 auto;
  margin-top:70rpx;
  background:#FF5151;
  border-radius:40rpx;
  font-size:32rpx;
}
.tiBtn.disabled{
   pointer-events: none;
   background:#929191
}
.phoneTitle{
  position: absolute;
  z-index: 99;
  font-size:20rpx;
  right:71rpx;
  top:246rpx;

}
/*遮罩*/
.mask{
    position: absolute;
    top:0;
    left:0;
    width:100%;
    height:100%;
    
    background:rgba(0, 0, 0, 0.26);
    z-index:999
}
.sucress{
    position:absolute;
    width:446rpx;
    height:480rpx;
    top:50%;
    left:50%;
    margin:-340rpx 0 0 -223rpx;
    background:#fff;
    border-radius: 10rpx;
    text-align:center;
}
.sucress.fails{
    height:340rpx;
    margin:-240rpx 0 0 -223rpx;
}
.sucress image{
    width:167rpx;
    height:192rpx;
    margin-top:48rpx;
}
.successText{
    text-align:center;
    font-size:30rpx;
    color:#333333;
    margin-top:25rpx;
}
.btnClick{
    width:350rpx;
    height:70rpx;
    text-align:center;
    margin:0 auto;
    background: #FF5151;
    border-radius: 40rpx;
    font-size: 30rpx;
    color:#fff;
    line-height: 70rpx;
    margin-top:50rpx;
}
.closes{
    position: absolute;
    width:61rpx;
    height:61rpx;
    background:#fff;
    border-radius: 50%;
    text-align:center;
    line-height:61rpx;
    font-size:30rpx;
    top:-30rpx;
    right:-30rpx
}
.closes image{
    width: 19rpx;
    height:19rpx;
    margin:0 auto;
}
.sucress image.fails{
   width:180rpx;
   height:158rpx;
}