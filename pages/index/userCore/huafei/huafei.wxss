}

.unLoginCoin{
    position: absolute;
    left:295rpx;
    top: 40rpx;
    height: 48rpx;
    line-height: 48rpx;
    font-size: 32rpx;
    font-weight: 500;
    color: rgb(70, 70, 70);
}
.boxInfo{
  height: 100rpx;
  position: relative;
  z-index:99
}
.userinfo-nickname{

    padding-top:30rpx;
    text-align:center;
    display: block;
    line-height: 1;

    font-size: 32rpx;
    font-weight:bold;
    color: #FFF;
    overflow: hidden;
}
.userinfo-coinImage{
    /* position: absolute; */
    margin-left:15rpx;
    width: 28rpx;
    height: 28rpx;
}
.coinView{
    /* position: absolute; */
    margin-left: 7rpx;
