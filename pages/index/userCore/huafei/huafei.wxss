    top:0;
    left:0;
}
/*头部样式 用户头像与用户名字*/
.titleUser{
    overflow: hidden;
}
.userinfo{
    height: 106rpx;
}
.box{
    float: left;
    width:320rpx;
}
.headImageBox{
    width: 109rpx;
    height: 109rpx;
    margin:0 auto;
    margin-top:40rpx;
}

.userinfo-headImage{
    /* position: absolute; */
   
    width: 106rpx;
    height: 106rpx;
    border-radius: 50%;
    position: relative;
    z-index:99
}
.headImage{
    position: absolute;
    width: 70rpx;
    height: 70rpx;
    left: 0rpx;
    top: 0rpx;
}
.userinfo-unLogin{
    font-size: 28rpx;
    height: 48rpx;
